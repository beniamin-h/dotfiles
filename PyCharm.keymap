<keymap version="1" name="PyCharm.keymap" parent="Eclipse">
  <action id="$Copy">
    <keyboard-shortcut first-keystroke="ctrl c" />
    <keyboard-shortcut first-keystroke="ctrl insert" />
    <keyboard-shortcut first-keystroke="shift ctrl c" />
  </action>
  <action id="$Paste">
    <keyboard-shortcut first-keystroke="ctrl v" />
    <keyboard-shortcut first-keystroke="shift insert" />
    <keyboard-shortcut first-keystroke="shift ctrl v" />
  </action>
  <action id="CopyPaths" />
  <action id="DatabaseView.CopyDdlAction">
    <keyboard-shortcut first-keystroke="shift ctrl alt g" />
  </action>
  <action id="Editor Copy">
    <keyboard-shortcut first-keystroke="shift ctrl c" />
  </action>
  <action id="Editor Paste">
    <keyboard-shortcut first-keystroke="shift ctrl v" />
  </action>
  <action id="PasteMultiple">
    <keyboard-shortcut first-keystroke="shift ctrl insert" />
  </action>
</keymap>
